public class Solution {
    public IList<int> PreorderTraversal(TreeNode root) {
        var result = new List<int>();
        if (root == null) return result;

        var stack = new Stack<TreeNode>();
        stack.Push(root);

        while (stack.Any()) {
            var cur = stack.Pop();
            result.Add(cur.val);

            if (cur.right != null) {
                stack.Push(cur.right);
            }

            if (cur.left != null) {
                stack.Push(cur.left);
            }
        }

        return result;
    }
}
